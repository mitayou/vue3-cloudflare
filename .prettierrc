{
  "singleQuote": true,
  "trailingComma": "none",
  "semi": false,
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx",
        "*.js",
        "*.jsx"
      ],
      "options": {
        "printWidth": 140,
        "tabWidth": 2,
        "useTabs": false,
        "semi": false,
        "singleQuote": true,
        "quoteProps": "as-needed",
        "jsxSingleQuote": false,
        "bracketSpacing": true,
        "arrowParens": "avoid"
      }
    }
  ]
}